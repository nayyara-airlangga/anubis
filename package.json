{
  "name": "snippets",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:reset": "yarn prisma migrate reset",
    "seed": "yarn prisma db seed",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "start-server-and-test 'yarn dev' 3000 cypress:open",
    "test:ci": "start-server-and-test 'yarn dev' 3000 cypress:run",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-all": "yarn check-format && yarn check-lint && yarn check-types",
    "postinstall": "husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "@prisma/migrate": "^3.9.1",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "crypto-es": "^1.2.7",
    "next": "12.0.10",
    "next-react-svg": "^1.1.3",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.4.1",
    "cypress-dotenv": "^2.0.0",
    "dotenv": "^16.0.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
